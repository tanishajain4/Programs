#include <iostream>

using namespace std;

int main()
{
    char a=65,b=65,c=65;
    int i,j,k;
    for(i=0;i<=2;i++)
    {   b=65;
    
        for(j=0;j<=2;j++)
        {  c=65;
         
            for(k=0;k<=2;k++)
            {   
                cout<<a<<b<<c;
                c++;
                cout<<" ";
            }
            b++;
        }
        a++;
    }

    return 0;
}
